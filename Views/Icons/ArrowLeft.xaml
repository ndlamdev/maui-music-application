<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="maui_music_application.ViewModels.IconViewModel"
             xmlns:viewModels="clr-namespace:maui_music_application.ViewModels"
             x:DataType="viewModels:IconViewModel"
             WidthRequest="{Binding WidthRequest}"
             HeightRequest="{Binding HeightRequest}">
    <Path
        Data="M20 12H4m0 0l6-6m-6 6l6 6"
        Aspect="{Binding Aspect}"
        HorizontalOptions="{Binding HorizontalOptions}"
        Stroke="{Binding Stroke}"
        StrokeLineCap="{Binding StrokeLineCap }"
        StrokeLineJoin="{Binding StrokeLineJoin}"
        StrokeThickness="{Binding StrokeThickness}" />
</ContentView>
