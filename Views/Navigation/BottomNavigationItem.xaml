<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Name="RootView"
             x:Class="maui_music_application.Views.Navigation.BottomNavigationItem">
    <VerticalStackLayout>
        <Image
            x:Name="ImageIcon"
            WidthRequest="40"
            HeightRequest="40" />
        <Label
            x:Name="Label"
            FontSize="15"
            FontAttributes="Bold"
            FontFamily="CenturyGothic" />
    </VerticalStackLayout>
</ContentView>